{
  "expo": {
    "name": "UnivTraze",
    "slug": "UnivTraze",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#28CD41"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.univtrazeapp.UnivTraze"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#28CD41"
      },
      "package": "com.univtrazeapp.UnivTraze"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
